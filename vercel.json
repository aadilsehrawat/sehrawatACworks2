{
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {"distDir": "staticfiles_build"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "website/wsgi.py"
        }
    ]
}
